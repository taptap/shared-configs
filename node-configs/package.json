{
  "name": "@taptap/node-configs",
  "version": "0.1.0",
  "description": "Shared configuration files for TapTap Node.js projects",
  "author": "kros@xd.com",
  "license": "MIT",
  "type": "commonjs",
  "homepage": "https://github.com/taptap/shared-configs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taptap/shared-configs.git",
    "directory": "node-configs"
  },
  "bugs": {
    "url": "https://github.com/taptap/shared-configs/issues"
  },
  "exports": {
    "./autocorrect": {
      "import": "./autocorrect/index.js",
      "require": "./autocorrect/index.cjs",
      "default": "./autocorrect/autocorrect.json"
    },
    "./biome": {
      "import": "./biome/index.js",
      "require": "./biome/index.cjs",
      "default": "./biome/biome.json"
    },
    "./markdownlint-cli2": {
      "import": "./markdownlint-cli2/index.js",
      "require": "./markdownlint-cli2/index.cjs",
      "default": "./markdownlint-cli2/markdownlint-cli2.jsonc"
    },
    "./prettier": {
      "import": "./prettier/index.js",
      "require": "./prettier/index.cjs",
      "default": "./prettier/prettierrc.json"
    }
  },
  "main": "index.js",
  "files": [
    "autocorrect",
    "biome",
    "markdownlint-cli2",
    "prettier"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  }
}
